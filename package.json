{
  "name": "gqlx-apollo-express-server",
  "version": "0.2.0",
  "description": "A Node.js Express middleware for integrating an Apollo server supporting gqlx.",
  "main": "dist",
  "types": "dist",
  "scripts": {
    "start": "ts-node src/example.ts",
    "test:unit": "jest src",
    "test": "npm run lint && npm run test:unit",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --config prettier.config.js --write 'src/**/*.ts'",
    "build": "tsc --project tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphql-extended/gqlx-apollo-express-server.git"
  },
  "keywords": [
    "graphql",
    "schema"
  ],
  "author": "graphql-extended <gqlx@anglevisions.com>",
  "engines": {
    "node": ">=8.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphql-extended/gqlx-apollo-express-server/issues"
  },
  "homepage": "https://gqlx.github.io",
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "apollo-upload-server": "^5.0.0",
    "graphql": "^0.13.2",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "^3.0.2",
    "graphql-type-json": "^0.2.1",
    "gqlx-js": "latest",
    "subscriptions-transport-ws": "^0.9.11"
  },
  "peerDependencies": {
    "express": "3.x || 4.x"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.11.1",
    "@types/graphql": "^0.13.1",
    "@types/graphql-type-json": "^0.1.2",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.6",
    "@types/ws": "^6.0.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.2",
    "jest": "^21.2.1",
    "jest-cli": "^23.6.0",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "ts-jest": "^21.1.2",
    "ts-node": "^5.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.1"
  }
}
